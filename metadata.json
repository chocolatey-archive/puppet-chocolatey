{
  "name": "chocolatey-chocolatey",
  "version": "1.2.0",
  "author": "chocolatey",
  "summary": "The Chocolatey package provider for Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/chocolatey/puppet-chocolatey",
  "project_page": "https://github.com/chocolatey/puppet-chocolatey",
  "issues_url": "https://github.com/chocolatey/puppet-chocolatey/issues",
  "description": "The Chocolatey package provider for Puppet",
  "tags": [
    "microsoft",
    "powershell",
    ".NET Framework",
    ".Net",
    "dot_net",
    "chocolatey",
    "package",
    "package manager"
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 2.7.0 < 2015.3.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2003",
        "Server 2003 R2",
        "Server 2008",
        "Server 2008 R2",
        "Server 2012",
        "Server 2012 R2",
        "7",
        "8",
        "10"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/powershell",
      "version_requirement": ">= 1.0.1 < 2.0.0"
    },
    {
      "name": "badgerious/windows_env",
      "version_requirement": ">= 2.2.1 < 3.0.0"
    }
  ]
}
